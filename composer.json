{
    "name": "innmind/signals",
    "type": "library",
    "description": "Manage multiple callbacks per signal",
    "keywords": ["cli", "signal", "handler"],
    "homepage": "http://github.com/Innmind/Signals",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Signals/issues"
    },
    "require": {
        "php": "~8.0",
        "innmind/immutable": "dev-next"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Signals\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Signals\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.1",
        "innmind/black-box": "^4.16",
        "innmind/coding-standard": "^1.1"
    }
}
